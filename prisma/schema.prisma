generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mysql"
  url      = "mysql://root:sirajudeen@127.0.0.1:3306/votingwebsite"
  relationMode = "prisma"

}

model User {
  id String @id @default(uuid())
  username String
  password String 
}

model Vote {

  id String @id @default(uuid())
  username String
  votename String
  description String

}

